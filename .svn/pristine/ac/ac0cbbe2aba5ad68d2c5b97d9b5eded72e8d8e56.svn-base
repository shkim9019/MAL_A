package com.mal_a.board;

import com.mal_a.cmn.DTO;

/**
 * @author sist133
 *
 */
public class BoardVO extends DTO {
	
	private String bno      ;/**글번호*/
	private String title  	;/**글제목*/
	private String cont 	;/**글내용*/
	private String cnt 		;/**조회수*/
	private String views    ;/**정지*/
	private String regId    ;/**등록자*/
	private String regDt    ;/**등록일*/
	private String modId    ;/**수정자*/
	private String modDt    ;/**수정일*/
	private String fileId   ;/**파일아이디*/
	
	public BoardVO() {}

	public BoardVO(String bno, String title, String cont, String cnt, String views, String regId, String regDt,
			String modId, String modDt, String fileId) {
		super();
		this.bno = bno;
		this.title = title;
		this.cont = cont;
		this.cnt = cnt;
		this.views = views;
		this.regId = regId;
		this.regDt = regDt;
		this.modId = modId;
		this.modDt = modDt;
		this.fileId = fileId;
	}

	public String getBno() {
		return bno;
	}

	public void setBno(String bno) {
		this.bno = bno;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getCont() {
		return cont;
	}

	public void setCont(String cont) {
		this.cont = cont;
	}

	public String getCnt() {
		return cnt;
	}

	public void setCnt(String cnt) {
		this.cnt = cnt;
	}

	public String getViews() {
		return views;
	}

	public void setViews(String views) {
		this.views = views;
	}

	public String getRegId() {
		return regId;
	}

	public void setRegId(String regId) {
		this.regId = regId;
	}

	public String getRegDt() {
		return regDt;
	}

	public void setRegDt(String regDt) {
		this.regDt = regDt;
	}

	public String getModId() {
		return modId;
	}

	public void setModId(String modId) {
		this.modId = modId;
	}

	public String getModDt() {
		return modDt;
	}

	public void setModDt(String modDt) {
		this.modDt = modDt;
	}

	public String getFileId() {
		return fileId;
	}

	public void setFileId(String fileId) {
		this.fileId = fileId;
	}

	@Override
	public String toString() {
		return "BoardVO [bno=" + bno + ", title=" + title + ", cont=" + cont + ", cnt=" + cnt + ", views=" + views
				+ ", regId=" + regId + ", regDt=" + regDt + ", modId=" + modId + ", modDt=" + modDt + ", fileId="
				+ fileId + ", toString()=" + super.toString() + "]";
	}



	
}
